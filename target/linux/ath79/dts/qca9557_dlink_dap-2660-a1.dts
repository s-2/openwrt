/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca955x.dtsi"

/ {
	model = "D-Link DAP-2660 A1";
	compatible = "dlink,dap-2660-a1", "qca,qca9557";

	aliases {
		led-boot = &led_power_green;
		led-failsafe = &led_power_red;
		led-running = &led_power_green;
		led-upgrade = &led_power_red;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power_red: power_red {
			label = "d-link:red:power";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		led_power_green: power_green {
			label = "d-link:green:power";
			gpios = <&gpio 23 GPIO_ACTIVE_LOW>;
		};
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "bootloader";
				reg = <0x0 0x40000>;
				read-only;
			};

			partition@40000 {
				label = "bdcfg";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				label = "rgdb";
				reg = <0x50000 0x10000>;
				read-only;
			};

			partition@60000 {
				label = "unknown";
				reg = <0x60000 0x10000>;
				read-only;
			};

			partition@70000 {
				label = "firmware";
				reg = <0x70000 0xee0000>;
				compatible = "wrg";
			};

			partition@f50000 {
				label = "langpack";
				reg = <0xf50000 0x20000>;
				read-only;
			};

			partition@f70000 {
				label = "captival";
				reg = <0xf70000 0x70000>;
				read-only;
			};

			partition@fe0000 {
				label = "certificate";
				reg = <0xfe0000 0x10000>;
				read-only;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x10000>;
				read-only;
			};
		};
	};
};

&uart {
	status = "okay";
};

&mdio0 {
	status = "okay";

	#phy-mask = <0x1>;

	phy0: ethernet-phy@4 {
		reg = <0x4>;
		phy-mode = "rgmii";
	};
};

&eth0 {
	status = "okay";

	pll-data = <0x96000000 0x94000101 0x94001313>;
	phy-handle = <&phy0>;

	gmac-config {
		device = <&gmac>;
		rgmii-enabled = <1>;
	};
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "pci168c,003c";
		reg = <0x0000 0 0 0 0>;
	};
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
};
