#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	aliases {
		led-boot = &led_power;
		led-failsafe = &led_status;
		led-running = &led_power;
		led-upgrade = &led_status;
	};

	leds {
		compatible = "gpio-leds";

		led_power: power {
			label = "d-link:green:power";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		led_status: status {
			label = "d-link:red:status";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		};
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x10000>;
				read-only;
			};

			partition@10000 {
				label = "nvram";
				reg = <0x10000 0x10000>;
				read-only;
			};

			partition@20000 {
				label = "firmware";
				reg = <0x20000 0x540000>;
				compatible = "denx,uimage";
			};

			partition@560000 {
				label = "linux4b";
				reg = <0x560000 0x290000>;
				read-only;
			};

			partition@610000 {
				label = "rootfsb";
				reg = <0x610000 0x1e0000>;
				read-only;
			};

			art: partition@7f0000 {
				label = "art";
				reg = <0x7f0000 0x10000>;
				read-only;
			};
		};
	};
};

&uart {
	status = "okay";
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
};
