. /lib/functions.sh

board=$(board_name)

case "$board" in
dlink,dap-2660-a1)
	uci set wireless.@wifi-device[0].disabled=0
	uci set wireless.@wifi-iface[0].ssid="dap2660"
	uci set wireless.@wifi-iface[0].key="dlinktest"
	uci set wireless.@wifi-iface[0].encryption="psk2+aes"
	uci commit wireless
	;;
esac

exit 0
